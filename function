#two choices

def mcAB(ans):
    score = 0
    ansC = str.upper(ans)
    scoreperchoice = 4/5
    c2w0 = scoreperchoice * 5
    c2w1 = scoreperchoice * 4 - scoreperchoice * 1
    c2w2 = scoreperchoice * 3 - scoreperchoice * 2
    c2w3 = 0
    c1w0 = scoreperchoice * 4 - scoreperchoice * 1
    c1w1 = scoreperchoice * 3 - scoreperchoice * 2
    c1w = 0
    c0w = 0
    if len(ansC) > 0:
        if ansC[0] == 'A':
            if len(ansC) >= 2:
                if  ansC[1] == 'B':
                    if len(ansC) >= 3:
                        if ansC[2] == 'C':
                            if len(ansC) >= 4:
                                if ansC[3] == 'D':
                                    if len(ansC) >= 5:
                                        if ansC[4] == 'E':
                                            score += c2w3
                                            return score
                                        else:
                                            score += c2w2
                                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                                            return score
                                    else:
                                        score += c2w2
                                        return score
                                if ansC[3] == 'E':
                                    score += c2w2
                                    return score
                                else:
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    score += c2w1
                                    return score
                            else:
                                score += c2w1
                                return score
                        if ansC[2] == 'D':
                            if len(ansC) >= 4:
                                if ansC[3] == 'E':
                                    score += c2w2
                                    return score
                                else:
                                    score += c2w1
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    return score
                        if ansC[2] == 'E':
                            score += c2w1
                            return score
                        else:
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            score += c2w0
                            return score
                    else:
                        score += c2w0
                        return score
                if ansC[1] == 'C':
                    if len(ansC) >= 3:
                        if ansC[2] == 'D':
                            if len(ansC) >= 4:
                                if ansC[3] == 'E':
                                    score += c1w
                                    return score
                                else:
                                    score += c1w
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    return score
                            else:
                                score += c1w
                                return score
                        if ansC[2] == 'E':
                            score += c1w
                            return score
                        else:
                            score += c1w1
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            return score
                    else:
                        score += c1w1
                        return score
                if ansC[1] == 'D':
                    if len(ansC) >= 3:
                        if ansC[2] == 'E':
                            score += c1w
                            return score
                        else:
                            score += c1w1
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            return score
                    else:
                        score += c1w1
                        return score
                if ansC[1] == 'E':
                    score += c1w1
                    return score
                else:
                    score += c1w0
                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                    return score
            else:
                score += c1w0
                return score
        if ansC[0] == 'B':
            if len(ansC) >= 2:
                if ansC[1] == 'C':
                    if len(ansC) >= 3:
                        if ansC[2] == 'D':
                            score += c1w
                            return score
                            #不檢查E
                        if ansC[2] == 'E':
                            score += c1w
                            return score
                        else:
                            score += c1w1
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            return score
                    else:
                        score += c1w1
                        return score
                if ansC[1] == 'D':
                    if len(ansC) >= 3:
                        if ansC[2] == 'E':
                            score += c1w
                            return score
                        else:
                            score += c1w1
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            return score
                    else:
                        score += c1w1
                        return score
                if ansC[1] == 'E':
                    score += c1w1
                    return score
                else:
                    score += c1w0
                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                    return score
        if ansC[0] == 'C':
            if len(ansC) >= 2:
                if ansC[1] == 'D':
                    if len(ansC) >= 3:
                        if ansC[2] == 'E':
                            score += c0w
                            return score
                        else:
                            score += c0w
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            return score
                    else:
                        score += c0w
                        return score
                if ansC[1] == 'E':
                    score += c0w
                    return score
                else:
                    score += c0w
                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                    return score
        if ansC[0] == 'D':
            score += c0w
            return score
        if ansC[0] == 'E':
            score += c0w
            return score
        else:
            score += c0w
            print("One of your answer is not 'A, B, C, D, or E'!!!")
            return score
    else:
        print("Your didn't type in your answer!")
        score += c0w
        return score

