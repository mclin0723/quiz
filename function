                                    score += c1w
                                    return score
                                else:
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    score += c1w
                                    return score
                            else:
                                score += c1w
                                return score
                        if ansC[2] == 'D':
                            if len(ansC) >= 4:
                                if ansC[3] == 'E':
                                    score += c2w2
                                    return score
                                else:
                                    score += c2w1
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    return score
                        if ansC[2] == 'E':
