                        return score
                if ansC[1] == 'E':
                    score += c1w1
                    return score
                else:
                    score += c1w0
                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                    return score
            else:
                score += c1w0
                return score
        if ansC[0] == 'B':
