# multiple choice(單選題)

def mcA(ans):
    score = 0
    ansC = str.upper(ans)
    if len(ansC) > 0:
        if ansC[0] == 'A':
            if len(ansC) > 1:
                return score
            score += 4
            return score
        return score
    return score

def mcB(ans):
    score = 0
    ansC = str.upper(ans)
    if len(ansC) > 0:
        if ansC[0] == 'B':
            if len(ansC) > 1:
                return score
            else:
                score += 4
                return score
        else:
            return score
    else:
        return score

