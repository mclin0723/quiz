def mcBCDE(ans):
    score = 0
    ansC = str.upper(ans)
    scoreperchoice = 4 / 5
    c4w0 = scoreperchoice * 5
    c4w1 = scoreperchoice * 4 - scoreperchoice * 1
    c3w0 = scoreperchoice * 4 - scoreperchoice * 1
    c3w1 = scoreperchoice * 4 - scoreperchoice * 1
    c2w0 = scoreperchoice * 4 - scoreperchoice * 1
    c2w = 0
    c1w = 0
    c0w = 0
    if len(ansC) > 0:
        if ansC[0] == 'A':
            if len(ansC) >= 2:
                if  ansC[1] == 'B':
                    if len(ansC) >= 3:
                        if ansC[2] == 'C':
                            if len(ansC) >= 4:
                                if ansC[3] == 'D':
                                    if len(ansC) >= 5:
                                        if ansC[4] == 'E':
                                            score += c4w1
                                            return score
                                        else:
                                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                                            score += c3w1
                                            return score
                                    else:
                                        score += c3w1
                                        return score
                                if ansC[3] == 'E':
                                    score += c3w1
                                    return score
                                else:
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    score += c2w
                                    return score
                            else:
                                score = c2w
                                return score
                        if ansC[2] == 'D':
                            if len(ansC) >= 4:
                                if ansC[3] == 'E':
                                    score += c3w1
                                    return score
                                else:
                                    print("One of your answer is not 'A, B, C, D, or E'!!!")
                                    score += c2w
                                    return score
                            else:
                                score = c2w
                                return score
                        if ansC[2] == 'E':
                            score += c2w
                            return score
                        else:
                            print("One of your answer is not 'A, B, C, D, or E'!!!")
                            score += c1w
                            return score
                    else:
                        score += c1w
                        return score
                if ansC[1] == 'C':
